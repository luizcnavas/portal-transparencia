# Ignore git metadata
.git
.gitignore

# Node modules and build caches
node_modules
npm-debug.log
pnpm-lock.yaml
.yarn
.yarnrc.yml

# Local env and IDE
.env
.env.*
!.env.example
.vscode
.idea
.DS_Store

# Tests and docs not needed in image
/tests
/docs

